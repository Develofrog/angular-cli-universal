{
  "name": "angular-cli-universal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server",
    "build": "npm run build:client",
    "build:client": "webpack",
    "build:server": "ngc -p tsconfig.server.json && webpack --progress --config webpack.server.config.js && rm -rf dist/out-tsc",
    "build:all": "npm run build:client && npm run build:server && rm -rf dist/out-tsc",
    "ngc:server": "ngc -p tsconfig.server.json",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "stylelint-staged": "stylelint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "4.0.0-rc.3",
    "@angular/compiler": "4.0.0-rc.3",
    "@angular/core": "4.0.0-rc.3",
    "@angular/forms": "4.0.0-rc.3",
    "@angular/http": "4.0.0-rc.3",
    "@angular/platform-browser": "4.0.0-rc.3",
    "@angular/platform-browser-dynamic": "4.0.0-rc.3",
    "@angular/platform-server": "4.0.0-rc.3",
    "@angular/router": "4.0.0-rc.3",
    "core-js": "2.4.1",
    "express": "4.15.2",
    "rxjs": "5.2.0",
    "zone.js": "0.8.0"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": "4.0.0-rc.3",
    "@types/express": "4.0.35",
    "@types/jasmine": "2.5.45",
    "@types/node": "7.0.8",
    "autoprefixer": "6.7.7",
    "codelyzer": "3.0.0-beta.3",
    "css-loader": "0.27.2",
    "cssnano": "3.10.0",
    "exports-loader": "0.6.4",
    "file-loader": "0.10.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "3.0.0",
    "lint-staged": "3.3.2",
    "pre-commit": "1.2.2",
    "postcss-loader": "1.3.3",
    "postcss-url": "5.1.2",
    "protractor": "5.1.1",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.3",
    "script-loader": "0.7.0",
    "source-map-loader": "0.2.0",
    "stylelint": "7.9.0",
    "stylelint-config-standard": "16.0.0",
    "style-loader": "0.13.2",
    "stylus-loader": "3.0.1",
    "ts-node": "2.1.0",
    "tslint": "4.5.1",
    "typescript": "2.2.1",
    "url-loader": "0.5.8",
    "webpack-dev-server": "2.4.1"
  },
  "greenkeeper": {
    "branchPrefix": "dep-update-",
    "label": "dep-update"
  },
  "lint-staged": {
    "*.ts": "tslint",
    "*.css": "stylelint"
  },
  "pre-commit": "lint-staged",
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
